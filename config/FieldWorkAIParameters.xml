<?xml version="1.0" encoding="utf-8" standalone="yes" ?>

<!--

This file stores all AI parameters data for the fieldwork driver.

	AIParameter(?).name is uses to get the AIParameter like: "a = self:getNamedParameter(name)"" 
	AIParameter(?).title is the name displayed above the selection field.
	AIParameter(?).min, .max and .inc can be used to generate a number selection setting, as alternative to direct declaration.

	AIParameter(?).Values.Value(?) are all values that can be assigned in the ai parameter.

	AIParameter(?).Texts.Text(?) are all text that can be assigned in the ai parameter, that are shown in the selection box.
-->
<AIParameters>
	<AIParameter name="workWidth" title="CP_ai_param_workWidth_title" min="1" max="50" incremental="0.1"/>
	<AIParameter name="centerMode" title="CP_ai_param_centerMode_title">
		<Values>
			<Value name="CENTER_MODE_UP_DOWN">1</Value> 
			<Value name="CENTER_MODE_SPIRAL">2</Value> 
			<Value name="CENTER_MODE_CIRCULAR">3</Value> 
			<Value name="CENTER_MODE_LANDS">4</Value> 
		</Values> 
		<Texts>
			<Text>CP_ai_param_centerMode_up_down</Text> 
			<Text>CP_ai_param_centerMode_spiral</Text> 
			<Text>CP_ai_param_centerMode_circular</Text> 
			<Text>CP_ai_param_centerMode_lands</Text> 
		</Texts> 
	</AIParameter>
	<AIParameter name="headlandCornerType" title="CP_ai_param_headlandCornerType_title">
		<Values>
			<Value name="HEADLAND_CORNER_TYPE_SMOOTH">1</Value> 
			<Value name="HEADLAND_CORNER_TYPE_SHARP">2</Value> 
			<Value name="HEADLAND_CORNER_TYPE_ROUND">3</Value> 
		</Values> 
		<Texts>
			<Text>CP_ai_param_headlandCornerType_smooth</Text> 
			<Text>CP_ai_param_headlandCornerType_sharp</Text> 
			<Text>CP_ai_param_headlandCornerType_round</Text> 
		</Texts> 
	</AIParameter>
	<AIParameter name="numberOfHeadlands" title="CP_ai_param_numberOfHeadlands_title" min="0" max="40"/>
	<AIParameter name="rowDirection" title="CP_ai_param_rowDirection_title">
		<Values>
			<Value name="ROW_DIRECTION_AUTOMATIC">5</Value> 
			<Value name="ROW_DIRECTION_LONGEST_EDGE">6</Value> 
		</Values> 
		<Texts>
			<Text>CP_ai_param_rowDirection_automatic</Text> 
			<Text>CP_ai_param_rowDirection_longest_edge</Text> 
		</Texts> 
	</AIParameter>
	<AIParameter name="startOnHeadland" title="CP_ai_param_startOnHeadland_title">
		<Values>
			<Value name="HEADLAND_START_ON_HEADLAND">1</Value> 
			<Value name="HEADLAND_START_ON_UP_DOWN_ROWS">3</Value> 
		</Values> 
		<Texts>
			<Text>CP_ai_param_startOnHeadland_on_headland</Text> 
			<Text>CP_ai_param_startOnHeadland_up_down_rows</Text> 
		</Texts> 
	</AIParameter>
</AIParameters>